{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "UseSqliteAsDatabaseEngine": false,
  "UsePostgresAsDatabaseEngine": true,
  "ConnectionStrings": {
    "SqliteDbContext": "Data Source=Database.db",
    "PostgresDbContext": "Server=localhost;Database=NicosiaAssessmentDb;Port=5432;User Id=postgres;Password=1234;"
  },
  "DockerComposeFileName": "docker-compose.yml",
  "Nicosia.Assessment.WebApi": {
    "BaseAddress": "Http://localhost:80"
  },
  "JwtSettings": {
    "Secret": "THIS IS USED TO SIGN AND VERIFY JWT TOKENS, REPLACE IT WITH YOUR OWN SECRET, IT CAN BE ANY STRING",
    "RefreshTokenTTL": 2
  }
}
